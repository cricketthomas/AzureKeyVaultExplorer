<Application
    x:Class="avalonia.kvexplorer.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sty="using:FluentAvalonia.Styling"
    xmlns:wnd="using:FluentAvalonia.UI.Windowing"
    Name="Key Vault Explorer">
    <!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->


	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://avalonia.kvexplorer/Resources/Resources.axaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>


	<Application.Styles>
        <!--<FluentTheme DensityStyle="Compact" />-->
        <sty:FluentAvaloniaTheme PreferUserAccentColor="True" UseSystemFontOnWindows="True" />
        <StyleInclude Source="/Resources/Styles.axaml" />
    </Application.Styles>
</Application>
